html(lang='ja')
  head
    meta(charset='utf-8')
    meta(name='viewport', content='width=device-width,initial-scale=1.0')
    link(rel="stylesheet" href="css/styles.css")
    link(rel='icon', type='image/ png', size='16x16', href='image/vcicon.png')
    title #{title}
  body
    header 
      li.TOP(id='top') ＴＯＰ
      li.TOPs ＞＞
      li.TOP(id='top' ) #{title}
    #app
      .A
        br
        h2 プログラム一覧
          br
        nav.menu(style='border-top: solid #1e366a 1px;border-bottom: solid #1e366a 1px;margin-left:39px')
            ul.list
              .D
                li.masuta(v-for="list in lists" v-on:mouseover="loadlist(list.method)" v-bind:id="list.id") {{list.value}}
              .F(v-show="is__select1")
                li.masuta2(v-for="masutalist in masuta" v-bind:id="masutalist.id") {{masutalist.value}}
              .F(v-show="is__select2")
                li.masuta2(v-for="Salelist in Sales" v-bind:id="Salelist.id") {{Salelist.value}}
              .F(v-show="is__select3")
                li.masuta2(v-for="Purchaselist in Purchase" v-bind:id="Purchaselist.id") {{Purchaselist.value}}
              .F(v-show="is__select4")
                li.masuta2(v-for="stocklist in Stock" v-bind:id="stocklist.id") {{stocklist.value}}
              .F(v-show="is__select5")
                li.masuta2(v-for="productionlist in Production" v-bind:id="productionlist.id") {{productionlist.value}}
              .F(v-show="is__select6")
                li.masuta2(v-for="transferlist in Transfer" v-bind:id="transferlist.id") {{transferlist.value}}
      br
      form.program(name='program' action='/tokuisaki1' method='POST')
        .B
          li.button(@click='kubun1' id='ku1') 入力/修正
          li.button3(@click='kubun3' id='ku3') 削除
          p.ku 現在の処理 : {{ kubun }}
          hr(style="margin-bottom:10px;margin-top:10px")
          label.koumoku
            | 得意先コード　：
            input.nyu(id='koumoku1' name='koumoku1' type='text', value=`${koumoku1}` placeholder='コードを入力') 
          label.koumoku
            | 請求締日　　　：
            input.nyu(id='koumoku10' type='text', v-bind:value='text', v-bind:readonly='is_readonly')
          br
          br
          label.koumoku
            | 得意先名１　　：
            input.nyu(id='koumoku2' name='koumoku2' type='text', value=`${koumoku2}`, v-bind:readonly='is_readonly')
          label.koumoku
            | 回収月　　　　：
            input.nyu(id='koumoku11' type='text', v-bind:value='text', v-bind:readonly='is_readonly')
          br
          br
          label.koumoku
            | 得意先名２　　：
            input.nyu(id='koumoku3' type='text', v-bind:value='text', v-bind:readonly='is_readonly')
          label.koumoku
            | 回収日　　　　：
            input.nyu(id='koumoku12' type='text', v-bind:value='text', v-bind:readonly='is_readonly')
          br
          br
          label.koumoku
            | 郵便番号　　　：
            input.nyu(id='koumoku4' type='text', v-bind:value='text', v-bind:readonly='is_readonly')
          label.koumoku
            | 税額通知　　　：
            input.nyu(id='koumoku13' type='text', v-bind:value='text', v-bind:readonly='is_readonly')
          br
          br
          label.koumoku
            | 住所１　　　　：
            input.nyu(id='koumoku5' type='text', v-bind:value='text', v-bind:readonly='is_readonly')
          label.koumoku
            | 売上端数　　　：
            input.nyu(id='koumoku14' type='text', v-bind:value='text', v-bind:readonly='is_readonly')
          br
          br
          label.koumoku
            | 住所２　　　　：
            input.nyu(id='koumoku6' type='text', v-bind:value='text', v-bind:readonly='is_readonly')
          label.koumoku
            | 消費税端数　　：
            input.nyu(id='koumoku15' type='text', v-bind:value='text', v-bind:readonly='is_readonly')
          br
          br
          label.koumoku
            | ＴＥＬ番号　　：
            input.nyu(id='koumoku7' type='text', v-bind:value='text', v-bind:readonly='is_readonly')
          label.koumoku
            | 請求先コード　：
            input.nyu(id='koumoku16' type='text', v-bind:value='text', v-bind:readonly='is_readonly')
          br
          br
          label.koumoku
            | ＦＡＸ番号　　：
            input.nyu(id='koumoku8' type='text', v-bind:value='text', v-bind:readonly='is_readonly')
          label.koumoku
            | 直送先コード　：
            input.nyu(id='koumoku17' type='text', v-bind:value='text', v-bind:readonly='is_readonly')
          br
          br
          label.koumoku
            | フリガナ　　　：
            input.nyu(id='koumoku9' type='text', v-bind:value='text', v-bind:readonly='is_readonly')
          label.koumoku
            | 直送先名　　　：
            input.nyu(id='koumoku18' type='text', v-bind:value='text', v-bind:readonly='is_readonly')
          br
          br
          hr
          br
          .C
            Button.button2(id='tokulist' type='tokulist') 更新
            Button.button2(id='sub1' type="submit") 送信
            Button.button2(id='reset') 中止
      .E(v-show='is__focus1')
        nav.menu
          ul.list
            table.table
              tr.tr 
                th.th #{midasi}
                th.th #{midasi2}
              each test in list
                tr.tr 
                  td.td #{test.id}
                  td.td #{test.user}
        img.footer(src='image/vc.png')
    script(src='https://cdn.jsdelivr.net/npm/vue/dist/vue.js')
    script(src='script/program.js')
    script(src='script/tokuisaki.js')
    script(src='script/information.js')